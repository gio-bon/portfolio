
<div class="bragdoc__entry">
  <div class="bragdoc__entry-milestone"></div>
  <div class="bragdoc__entry-block">
    <span class="bragdoc__entry-date">
        {{ entry.data.date | dateDisplay   }}
    </span>
    <br/>
    <h2 class="bragdoc__entry-title"><span class="bragdoc__icon">{{ entry.data.icon }}</span> {{ entry.data.title }}</h2>
    <div class="bragdoc__entry-content">
        {{ entry.templateContent | safe }}
    </div>
  </div>
  <div class="bragdoc__taglist">
  {% for category in entry.data.categories %}
    <span># {{category}}</span>
  {% endfor %}
  </div>
</div>
